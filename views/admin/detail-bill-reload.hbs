
                    <section class="example">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover">
                                <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Product Name</th>
                                    <th>Size</th>
                                    <th>Quantity</th>
                                    <th>Total Price</th>
                                    <th>Order Date</th>
                                    <th>Status</th>

                                </tr>
                                </thead>
                                {{#each this.list}}
                                    <tbody>
                                    <tr>
                                        <td>{{this.OrderID}}</td>
                                        <td>{{this.ProName}}</td>
                                        <td>{{this.Size}}</td>
                                        <td>{{this.Amount}}</td>
                                        <td>{{format_number this.Total}}</td>
                                        <td>{{time this.OrderDate}}</td>
                                        <td>{{this.Name}}</td>
                                    </tr>
                                    </tbody>
                                {{/each}}
                            </table>
                        </div>
                    </section>